<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ikumb.edugate">

    <application
        android:name=".EdugateApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.after_register.AfterRegisterActivity"></activity>
        <activity android:name=".ui.entry.EntryActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.loginregister.LoginActivity"></activity>
        <activity
            android:name=".ui.main.MainActivity"
            android:screenOrientation="unspecified" />
    </application>

</manifest>